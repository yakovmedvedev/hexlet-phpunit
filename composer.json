{
  "name": "hexlet/php-package",
  "version": "0.0.1",
  "bin": [
    "bin/php-package"
  ],
  "type": "library",
  "authors": [
    {
      "name": "Kirill Mokevnin",
      "email": "mokevnin@gmail.com"
    }
  ],
  "description": "hexlet php-package",
  "license": "MIT",
  "scripts": {
    "test": "phpunit tests"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/",
      "App\\Classes\\": "src/classes",
      "App\\Classes\\User\\": "src/classes/user/"
    },
    "files": [
    "src/Utils.php",
    "src/SetArray.php"
]
  },
  "autoload-dev": {
    "psr-4": {
      "Hexlet\\Phpunit\\Tests\\": "tests"
    }
  },
  "require": {
    "symfony/string": "^7.2",
    "illuminate/collections": "^11.41",
    "nesbot/carbon": "^3.8",
    "hexlet/trees":"dev-main",
    "php-ds/php-ds": "^1.7"
  },
      "repositories":[
        {
            "type":"git",
            "url":"https://github.com/hexlet-components/php-immutable-fs-trees.git"
        }
    ],
  "require-dev": {
    "psy/psysh": "@stable",
    "phpstan/phpstan-phpunit": "^2.0",
    "phpstan/extension-installer": "^1.3",
    "symfony/var-dumper": "^7.2",
    "squizlabs/php_codesniffer": "*",
    "phpstan/phpstan": "^2.1",
    "phpunit/phpunit": "^11.5"
  },
  "config": {
    "allow-plugins": {
      "phpstan/extension-installer": true
    }
  }
}
